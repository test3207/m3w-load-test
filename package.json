{
  "name": "m3w-load-test",
  "version": "0.1.0",
  "description": "Load testing suite for M3W",
  "private": true,
  "scripts": {
    "setup": "node scripts/setup.cjs",
    "test:full": "node scripts/run-test.cjs",
    "test:quick": "node scripts/run-test.cjs --skip-k6",
    "test:capacity": "k6 run k6/capacity.js",
    "monitor": "node scripts/monitor.cjs",
    "seed": "node scripts/seed.cjs",
    "cleanup": "node scripts/cleanup.cjs",
    "cleanup:full": "node scripts/cleanup.cjs --full",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down -v",
    "docker:logs": "docker compose logs -f",
    "podman:up": "podman-compose up -d",
    "podman:down": "podman-compose down -v",
    "podman:logs": "podman-compose logs -f"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}
